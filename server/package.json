{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npx nodemon",
    "prestart": "npm run build",
    "build": "tsc",
    "build:proto": "npx grpc_tools_node_protoc --proto_path=./src/proto/ --plugin=protoc-gen-grpc=node_modules/.bin/grpc_tools_node_protoc_plugin --grpc_out=./src/generated --js_out=import_style=commonjs,binary:./src/generated --ts_out=./src/generated ./src/proto/movies.proto"
  },
  "devDependencies": {
    "@types/node": "20.1.0",
    "grpc-tools": "^1.12.4",
    "nodemon": "2.0.22",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@grpc/proto-loader": "0.7.7",
    "@types/google-protobuf": "3.15.6",
    "dotenv": "16.0.3",
    "google-protobuf": "3.21.2",
    "grpc": "1.24.11",
    "mongodb": "5.4.0",
    "protobufjs": "7.2.3",
    "ts-protoc-gen": "0.15.0",
    "yup": "1.1.1"
  }
}
